# @PACKAGE@.conf - @DESCRIPTION@ #

# Terminal Color Support #
COLOR="yes";

# Language Support #
LC_ALL="C"
LANG="C"

# Log Date Format #
DATEFORMAT="+%D-%H:%M:%S";

# @PACKAGE@ PATHS #

# PLUGINS DATABASE PATH #
PLUGINSDIR="@localstatedir@/lib/@PACKAGE@/plugins";

# HOSTS DATABASE PATH #
HOSTSDIR="@localstatedir@/lib/@PACKAGE@/hosts";

# SSH HOSTS ACCESS DATABASE PATH #
SSHHOSTSDIR="@localstatedir@/lib/@PACKAGE@/sshhosts";
